{
  "name": "@3dverse/livelink.core",
  "version": "1.1.3",
  "description": "3dverse Livelink Core types.",
  "main": "./dist/library.cjs",
  "module": "./dist/library.mjs",
  "typings": "./dist/sources/library.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "=============== Build =================": "",
    "prebuild": "node ci/auto-generate.js ./ftl-schemas/ ci/ ./_prebuild/engine_types/",
    "build": "run-p build:*",
    "build:esbuild": "node esbuild.js",
    "build:tsc": "tsc",
    "build:tsc-lib": "tsc --project tsconfig.lib.json",
    "build:lint": "eslint sources _prebuild",
    "clean": "rimraf dist out",
    "=============== Spec =================": "",
    "spec:bundle": "npx @asyncapi/cli bundle -d ./spec ./livelink.yaml -o ./spec/livelink-core-1-0.yaml",
    "=============== Dev =================": "",
    "predev": "npm link",
    "dev": "run-p dev:*",
    "dev:watch": "node esbuild.js dev",
    "dev:ts": "tsc --watch",
    "dev:ts-lib": "tsc --watch --project tsconfig.lib.json",
    "dev:doc": "typedoc --watch"
  },
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.5.3",
    "esbuild": "^0.20.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "typedoc": "^0.27.5",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.18.2",
    "xxhashjs": "^0.2.2"
  }
}
